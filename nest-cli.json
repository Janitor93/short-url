{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/short-url/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/short-url/tsconfig.app.json",
    "assets": [
      "**/*.proto"
    ],
    "watchAssets": true
  },
  "monorepo": true,
  "root": "apps/short-url",
  "projects": {
    "short-url": {
      "type": "application",
      "root": "apps/short-url",
      "entryFile": "main",
      "sourceRoot": "apps/short-url/src",
      "compilerOptions": {
        "tsConfigPath": "apps/short-url/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "grpc": {
      "type": "library",
      "root": "libs/grpc",
      "entryFile": "index",
      "sourceRoot": "libs/grpc/src",
      "compilerOptions": {
        "tsConfigPath": "libs/grpc/tsconfig.lib.json"
      }
    }
  }
}